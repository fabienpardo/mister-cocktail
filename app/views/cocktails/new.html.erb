<div class="fullpage">
  <div class="content text-center">
    <h1 class="title_page">Nouvelle mixture</h1>
      <%= form_for(@cocktail, class: "form-group", multipart: true) do |f| %>
      <%= f.label 'Nom', class: 'control-label' %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.submit class: 'btn btn-primary', style: 'margin: 50px' %>

      <%= f.label :picture %>
      <%= f.file_field :picture %>
      <% end %>
  </div>
</div>